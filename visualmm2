        if selectedWeaponName == "" then

            WindUI:Notify({

                Title = "Weapon Dupe Error",

                Content = "Please enter a weapon name!",

                Icon = "x-circle",

                Duration = 5,

            })

            return

        end

        

        WindUI:Notify({

            Title = "Weapon Dupe",

